{
  "manifest_version": 3,
  "name": "Phishing URL Detector",
  "version": "1.0",
  "description": "Detects potentially malicious or phishing URLs using machine learning",
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "contextMenus",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Phishing URL Detector"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
